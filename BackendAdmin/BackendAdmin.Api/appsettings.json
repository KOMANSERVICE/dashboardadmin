{
  "ConnectionStrings": {
    "DashAdminDatabase": "DataBaseLocal"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Vault": {
    "PathMountPoint": "dashadmin",
    "MountPoint": "secret"
  },
  "Allow": {
    "Origins": "CorsAllowedOrigins"
  },
  "JWT": {
    "ValidIssuer": "JWTValidAudienceApi",
    "ValidAudience": "JWTValidIssuerApi",
    "Secret": "JWTSecretApi"
  }
}
